Workflow sandwitch {
	Resources { bread 1x; butter 2x; final sandwich 1x;}
	Node Amol
	{
		input bread 4;
		output bread_temp 5;
	}
	Node Amol2
	{
		input bread_temp 5;
	    input butter 3;
		final output sandwich 3;
	}
	Connection{
		Node Amol -> Node Amol2 {bread_temp 5;};
	}
	func Amol{
		convert bread(1) bread_temp(1) { rate 5; quantity 5; print "amok";};
	}
}

